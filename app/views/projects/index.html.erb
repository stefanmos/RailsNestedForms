<div class="col-12-12">
  <h3>Projects Index</h3>
  <%= link_to 'New Project', new_project_path, :class => 'button-left' %>
</div>

<% @project.each do |project| %>
    <div class="col-12-12">
      <div class="task">
        <h4><%= project.project_name %></h4>
        <%= link_to 'View', edit_project_path(project.id) %>
        <%= link_to 'Delete', project, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
    </div>
<% end %>
